{
  "name": "app",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "run-s lint:prettier lint:eslint check",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check --log-level silent src",
    "lint:tsc": "tsc --noEmit",
    "format": "run-s format:prettier format:eslint",
    "format:eslint": "eslint . --fix",
    "format:prettier": "prettier --write --log-level silent src"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.415.0",
    "@aws-sdk/util-dynamodb": "^3.415.0",
    "@felte/validator-zod": "^1.0.17",
    "@sendgrid/mail": "^7.7.0",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "felte": "^1.2.12",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.38.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.25.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "autoprefixer": "^10.4.15",
    "cssnano": "^6.0.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-svelte": "^2.33.2",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "postcss": "^8.4.30",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.4",
    "yarn-run-all": "^3.1.1"
  }
}
